{
    "name": "elementable-vue",
    "version": "0.4.0",
    "description": "The elementable component for Vue js",
    "author": {
        "name": "Sander Maas",
        "email": "sandermaas7@hotmail.com"
    },
    "repository": "https://github.com/sandermaas/elementable-vue",
    "bugs": "https://github.com/sandermaas/elementable-vue/issues",
    "keywords": [
        "Vue",
        "elementable",
        "elementable-vue",
        "datatable"
    ],
    "license": "MIT",
    "private": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
